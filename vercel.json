{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["public/**"]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/public/index.html"
		},
		{
			"src": "/api/upload-model",
			"dest": "/api/upload-model.js"
		},
		{
			"src": "/api/get-models",
			"dest": "/api/get-models.js"
		}
	]
}
